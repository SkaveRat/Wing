{
	"name": "Wing Twitter client",
	"version": "0.0.1",
	"manifest_version": 1,
	"description": "Testproject for Twitter API",
	"options_page": "options.html",
	"permissions": [
		"https://api.twitter.com/",
		"https://userstream.twitter.com/"
	],
	"background": {
		"scripts": [
			"lib/jquery-1.7.2.min.js",
			"lib/WingBackend.js",
			"lib/oauth.js",
			"lib/sha1.js",
			"background.js"
		]
	},
	"app": {
		"launch": {
			"local_path": "index.html"
		}
	}
}